<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearForgetPasword"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageArrowleft"
            android:layout_height="@dimen/_11pxh"
            android:layout_marginEnd="@dimen/_38pxh"
            android:layout_marginStart="@dimen/_38pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="@dimen/_15pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowleft"
            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
        <ImageView android:id="@+id/imageRectangleSix"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_177pxh"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="@dimen/_213pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_rectangle6}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle6"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtFrameSeven"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_38pxh"
            android:layout_marginStart="@dimen/_38pxh"
            android:layout_marginTop="@dimen/_41pxv"
            android:layout_width="wrap_content"
            android:paddingTop="@dimen/_1pxv" android:singleLine="true"
            android:text="@string/msg_forget_password"
            style="@style/txtSolid" tools:text="@string/msg_forget_password"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtEntreyourEmai"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_49pxh"
            android:layout_marginStart="@dimen/_49pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/msg_entre_your_emai"
            style="@style/txtRobotoromanregular14_1" tools:text="@string/msg_entre_your_emai"/>
        <FrameLayout android:id="@+id/frameStackcreatefromfra"
            android:layout_height="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_38pxh"
            android:layout_marginStart="@dimen/_38pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="@dimen/_40pxh" style="@style/groupStylewhite_A700">
            <View android:background="@color/white_A700"
                android:id="@+id/viewCreatefromfra"
                android:layout_gravity="start"
                android:layout_height="@dimen/_18pxv"
                android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_40pxh"/>

            <TextView
                android:id="@+id/txtEmail"
                style="@style/txtRobotoromanmedium16"
                android:layout_width="45dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:ellipsize="end"
                android:gravity="top|start"
                android:singleLine="true"
                android:text="@string/lbl_email"
                tools:text="@string/lbl_email" />
        </FrameLayout>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_border_gray_100_radius_4"
            android:ellipsize="end"
            android:hint="@string/lbl_mail_mail_com"
            android:id="@+id/etEmailOne"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_6pxh"
            android:layout_marginStart="@dimen/_38pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_10pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_13pxv" android:singleLine="true"
            android:textColorHint="@color/gray_400_a2"
            style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSend"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_106pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_11pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_11pxv" android:singleLine="true"
            android:text="@string/lbl_send" android:textAllCaps="false"
            style="@style/btnSolidRounded_3" tools:text="@string/lbl_send"/>
    </LinearLayout>
</layout>
