                android:id="@+id/linearColumnthree"
                style="@style/groupStylelight_green_2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.12"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtThree"
                    style="@style/txtRobotoromanmedium15_2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_3"
                    tools:text="@string/lbl_3" />
            </LinearLayout>

            <View
                android:id="@+id/viewLineTwo"
                android:layout_width="@dimen/_24pxh"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_marginBottom="@dimen/_15pxv"
                android:background="@color/light_green_A700" />

            <LinearLayout
                android:id="@+id/linearColumnfour"
                style="@style/groupStylewhite_4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.12"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtFour"
