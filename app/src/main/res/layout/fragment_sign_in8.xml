                        android:id="@+id/etStartDateFormation"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:paddingBottom="@dimen/_12pxv"
                        android:paddingEnd="@dimen/_23pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_12pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_dd_mm_yyyy"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_5"
                        tools:text="@string/lbl_dd_mm_yyyy"
                        android:inputType="date" />
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearList05"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEndDate"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_end_date"
                        style="@style/txtRobotosemibolditalic17_1"
                        tools:text="@string/lbl_end_date"/>
                    <EditText
                        android:ellipsize="end"
                        android:gravity="center"
                        android:id="@+id/etEndDateFormation"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_9pxv"
