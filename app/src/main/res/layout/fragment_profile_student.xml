                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageUserOne"
                            android:layout_width="@dimen/_24pxh"
                            android:layout_height="@dimen/_23pxh"
                            android:layout_gravity="center"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user_black_900_23x24"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_user_black_900_23x24" />

                        <TextView
                            android:id="@+id/txtMessages"
                            style="@style/txtRobotoregular12"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:letterSpacing="0.02"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_messages"
                            tools:text="@string/lbl_messages" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearColumnvectorOne"
                        style="@style/groupStylewhite_8"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageVectorOne"
                            android:layout_width="@dimen/_16pxh"
                            android:layout_height="@dimen/_22pxh"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/txtProfile"
                            style="@style/txtRobotoregular12_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:letterSpacing="0.02"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_profile"
                            tools:text="@string/lbl_profile" />
                    </LinearLayout>
