                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.54"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_about_me"
                        tools:text="@string/lbl_about_me" />

                </LinearLayout>

                <TextView
                    android:id="@+id/txtDescription"
                    style="@style/txtRobotoromanregular15"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:gravity="center_horizontal"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:text="@string/msg_i_know_i_can_he"
                    tools:text="@string/msg_i_know_i_can_he" />

                <LinearLayout
                    android:id="@+id/linearTitle4"
                    style="@style/groupStylecornerRadius_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_17pxh"
