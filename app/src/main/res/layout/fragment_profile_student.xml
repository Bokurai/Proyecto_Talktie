                            tools:src="@drawable/img_edit" />
                    </FrameLayout>
                </FrameLayout>

                <TextView
                    android:id="@+id/txtRababBeihaqi"
                    style="@style/txtPoppinssemibold22_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:lineSpacingExtra="@dimen/_11pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_rabab_beihaqi2"
                    tools:text="@string/lbl_rabab_beihaqi2" />
            </LinearLayout>
        </FrameLayout>

    </LinearLayout>
    <androidx.core.widget.NestedScrollView
        android:clipToPadding="false"
        android:fillViewport="true"
        android:id="@+id/scrollViewScrollview"
        android:layout_height="0dp" android:layout_weight="1"
        android:layout_width="match_parent">
        <FrameLayout android:id="@+id/frameModificarperfi"
            android:layout_height="match_parent"
            android:layout_width="match_parent">

