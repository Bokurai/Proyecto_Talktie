<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SignIn4"
    android:gravity="center_horizontal"
    android:id="@+id/frameStack375x812"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="horizontal">
    <LinearLayout android:id="@+id/linearSinginFour"
        android:layout_gravity="start"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linear11ChooseLocat"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameMap"
                android:layout_height="match_parent"
                android:layout_width="match_parent" style="@style/groupStylegray_400_30">
                <TextView android:ellipsize="end"
                    android:id="@+id/txt375x812"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_301pxv"
                    android:layout_marginEnd="@dimen/_112pxh"
                    android:layout_marginStart="@dimen/_112pxh"
                    android:layout_marginTop="@dimen/_301pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_375_x_812"
                    style="@style/txtRobotosemibolditalic48" tools:text="@string/lbl_375_x_812"/>

                <FrameLayout
                    android:id="@+id/frameStacksearch"
                    android:layout_width="444dp"
                    android:layout_height="682dp"
                    android:layout_gravity="bottom|start"
                    android:layout_marginTop="@dimen/_10pxv">

                    <ImageView
                        android:id="@+id/imageImagenSeven"
                        android:layout_width="510dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="start"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_imagen7"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_imagen7" />

                    <LinearLayout
                        android:id="@+id/linearColumnsearch"
                        android:layout_width="match_parent"
                        android:layout_height="600dp"
                        android:layout_gravity="top|center"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:orientation="vertical">

                        <androidx.appcompat.widget.SearchView
                            android:id="@+id/searchViewSearch"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/rectangle_bg_white_a700_radius_8"
                            android:gravity="start"
                            android:theme="@style/searchViewSearch"
                            app:iconifiedByDefault="false"
                            app:queryBackground="@android:color/transparent"
                            app:queryHint="@string/msg_type_location_y"/>

                        <FrameLayout
                            android:id="@+id/frameButtonSet"
                            android:layout_width="@dimen/_125pxh"
                            android:layout_height="@dimen/_35pxv"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/_109pxh"
                            android:layout_marginTop="@dimen/_213pxv"
                            android:layout_marginEnd="@dimen/_109pxh">

                            <ImageView
                                android:id="@+id/imageBackground"
                                android:layout_width="@dimen/_125pxh"
                                android:layout_height="@dimen/_35pxh"
                                android:layout_gravity="start"
                                android:scaleType="fitXY"
                                tools:ignore="ContentDescription" />

                            <LinearLayout
                                android:id="@+id/linearRowsetlocation"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="top|center"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/txtSetLocation"
                                    style="@style/txtRobotoromanmedium15_1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.93"
                                    android:ellipsize="end"
                                    android:lineSpacingExtra="@dimen/_3pxh"
                                    android:singleLine="true"
                                    android:text="@string/lbl_set_location"
                                    tools:text="@string/lbl_set_location" />

                                <ImageView
                                    android:id="@+id/imageArrowright"
                                    android:layout_width="@dimen/_6pxh"
                                    android:layout_height="@dimen/_9pxh"
                                    android:layout_marginStart="@dimen/_14pxh"
                                    android:layout_marginTop="@dimen/_5pxv"
                                    android:layout_marginBottom="@dimen/_3pxv"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_arrowright"
                                    tools:ignore="ContentDescription"
                                    tools:src="@drawable/img_arrowright" />
                            </LinearLayout>
                        </FrameLayout>

                        <ImageView
                            android:id="@+id/imageLinkedin"
                            android:layout_width="@dimen/_26pxh"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_gravity="start"
                            android:layout_marginStart="@dimen/_159pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_marginEnd="@dimen/_159pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_linkedin_black_900"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_linkedin_black_900" />
                    </LinearLayout>
                </FrameLayout>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearNavBars"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStyle">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_marginStart="@dimen/_9pxh"
                            android:layout_marginTop="@dimen/_18pxv"
                            android:layout_width="@dimen/_13pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtYourLocation"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_marginEnd="@dimen/_134pxh"
                            android:layout_marginStart="@dimen/_148pxh"
                            android:layout_marginTop="@dimen/_18pxv"
                            android:layout_weight="0.89"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@string/lbl_your_location"
                            style="@style/txtRobotoromanbold17_1" tools:text="@string/lbl_your_location"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:backgroundTint="@color/light_green_A700"
        android:id="@+id/floatingBtnFloatingactionbutton"
        android:layout_gravity="bottom|end"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_22pxv"
        android:layout_marginEnd="@dimen/_34pxh"
        android:layout_marginStart="@dimen/_34pxh"
        android:layout_marginTop="@dimen/_22pxv"
        android:layout_width="wrap_content"
        app:fabCustomSize="@dimen/_56pxh"
        app:srcCompat="@drawable/img_contrast" app:tint="@color/white_A700"/>
</LinearLayout>
