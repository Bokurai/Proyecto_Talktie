<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SignIn10"
    android:gravity="center_horizontal"
    android:id="@+id/linearSinginTen"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    style="@style/groupStylewhite_A700">
    <FrameLayout android:gravity="start"
        android:id="@+id/frame17JobPreferen"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">

        <LinearLayout
            android:id="@+id/linearColumnone"
            style="@style/groupStylewhite_A700"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            android:layout_marginEnd="@dimen/_10pxh"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearNumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_72pxv"
                android:layout_marginEnd="@dimen/_40pxh"
                android:gravity="end"
                android:orientation="horizontal">


                <LinearLayout
                    android:id="@+id/linearColumnZero"
                    style="@style/groupStylelight_green_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtOne"
                        style="@style/txtRobotoromanmedium15_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.12"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_13pxh"
                        android:paddingBottom="@dimen/_7pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_1"
                        android:textAlignment="center"
                        tools:text="@string/lbl_1" />
                </LinearLayout>


                <View
                    android:id="@+id/viewLine"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:background="@color/light_green_A700" />

                <LinearLayout
                    android:id="@+id/linearColumntwo"
                    style="@style/groupStylelight_green_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtTwo"
                        style="@style/txtRobotoromanmedium15_2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_2"
                        tools:text="@string/lbl_2" />
                </LinearLayout>

                <View
                    android:id="@+id/viewLineOne"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:background="@color/light_green_A700" />

                <LinearLayout
                    android:id="@+id/linearColumnthree"
                    style="@style/groupStylelight_green_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtThree"
                        style="@style/txtRobotoromanmedium15_2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_3"
                        tools:text="@string/lbl_3" />
                </LinearLayout>
                <View
                    android:id="@+id/viewLineTwo"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:background="@color/light_green_A700" />

                <LinearLayout
                    android:id="@+id/linearColumnfour"
                    style="@style/groupStylelight_green_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtFour"
                        style="@style/txtRobotoromanmedium15_2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_4"
                        tools:text="@string/lbl_4" />
                </LinearLayout>
                <View
                    android:id="@+id/viewLineThree"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:background="@color/light_green_A700" />

                <LinearLayout
                    android:id="@+id/linearColumnfive"
                    style="@style/groupStylelight_green_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btnMaximize"
                        android:layout_width="@dimen/_32pxh"
                        android:layout_height="@dimen/_32pxh"
                        android:background="@drawable/rectangle_bg_light_green_a700_radius_16"
                        android:paddingHorizontal="@dimen/_9pxh"
                        android:paddingVertical="@dimen/_9pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_maximize_white_a700"
                        tools:ignore="ContentDescription" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearColumnjobcategories"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:gravity="start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtJobCategories"
                    style="@style/txtRobotosemibolditalic17_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="Availability"
                    />

                <EditText
                    android:id="@+id/etJobPreferences"
                    style="@style/etSolidRounded_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/lbl_art_availability"
                    android:inputType="text"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_12pxv"
                    android:singleLine="true"
                    android:textColor="@android:color/black"
                    android:textColorHint="@color/bluegray_400"
                    tools:ignore="Autofill" />

            </LinearLayout>

            <FrameLayout
                android:id="@+id/frameButtonContinue"
                android:layout_width="@dimen/_311pxh"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_219pxv"
                android:layout_marginEnd="@dimen/_13pxh">


                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnDone"
                    style="@style/btnSolidRounded_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_done"
                    android:textAllCaps="false"
                    tools:text="@string/lbl_done" />

            </FrameLayout>
        </LinearLayout>
        <androidx.appcompat.widget.Toolbar
            android:layout_marginStart="@dimen/_10pxh"
            android:id="@+id/toolbarToolbar"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">

            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">

                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_black_900"
                    tools:ignore="ContentDescription" />
                <LinearLayout
                    android:id="@+id/linearRowlocation"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_80pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.94"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtJobPreferences"
                        style="@style/txtRobotosemibolditalic17_2"
                        android:layout_width="174dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginStart="50dp"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_job_preferences" />
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
    </FrameLayout>
</LinearLayout>
