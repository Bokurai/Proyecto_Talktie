<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SignIn10"
    android:gravity="center_horizontal"
    android:id="@+id/linearSinginTen"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    style="@style/groupStylewhite_A700">
    <FrameLayout android:gravity="start"
        android:id="@+id/frame17JobPreferen"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnone"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="end"
                android:id="@+id/linearNumber"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_71pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOne"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_13pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_1"
                    style="@style/txtSolidRounded_2" tools:text="@string/lbl_1"/>
                <View android:background="@color/light_green_A700"
                    android:id="@+id/viewLine"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_24pxh"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumntwo"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylelight_green_2">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtTwo"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_2"
                        style="@style/txtRobotoromanmedium15_1" tools:text="@string/lbl_2"/>
                </LinearLayout>
                <View android:background="@color/light_green_A700"
                    android:id="@+id/viewLineOne"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_24pxh"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnthree"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylelight_green_2">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtThree"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_3"
                        style="@style/txtRobotoromanmedium15_1" tools:text="@string/lbl_3"/>
                </LinearLayout>
                <View android:background="@color/light_green_A700"
                    android:id="@+id/viewLineTwo"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_24pxh"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnfour"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylelight_green_2">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtFour"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_4"
                        style="@style/txtRobotoromanmedium15_1" tools:text="@string/lbl_4"/>
                </LinearLayout>
                <View android:background="@color/light_green_A700"
                    android:id="@+id/viewLineThree"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_24pxh"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_light_green_a700_radius_16"
                    android:id="@+id/btnMaximize"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_width="@dimen/_32pxh"
                    android:paddingHorizontal="@dimen/_9pxh"
                    android:paddingVertical="@dimen/_9pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_maximize_white_a700" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnjobcategories"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJobCategories"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_job_categories"
                    style="@style/txtRobotosemibolditalic17_1" tools:text="@string/lbl_job_categories"/>
                <EditText
                    android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/lbl_art_director"
                    android:id="@+id/etGroup124"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/black_900"
                    style="@style/etSolidRounded_3" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPayDay"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_pay_day"
                    style="@style/txtRobotosemibolditalic17_1" tools:text="@string/lbl_pay_day"/>
                <Spinner
                    android:background="@drawable/layer_list_bg_bluegray_50_radius_8"
                    android:id="@+id/spinnerGroup123"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent" />
                <LinearLayout android:id="@+id/linearRowmin"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearList04"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMin"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_min"
                            style="@style/txtRobotosemibolditalic17_1" tools:text="@string/lbl_min"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                            android:ellipsize="end"
                            android:hint="@string/lbl_1000"
                            android:id="@+id/etPrice"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRounded_3" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearList05"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMax"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_max"
                            style="@style/txtRobotosemibolditalic17_1" tools:text="@string/lbl_max"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                            android:ellipsize="end"
                            android:hint="@string/lbl_8000"
                            android:id="@+id/etPriceOne"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRounded_3" tools:ignore="Autofill"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameButtonContinue"
                android:layout_gravity="end"
                android:layout_height="@dimen/_50pxv"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_219pxv" android:layout_width="@dimen/_311pxh">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtContinue"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_continue"
                    style="@style/txtRobotosemibolditalic17" tools:text="@string/lbl_continue"/>
                <FrameLayout android:id="@+id/frameButtonSelect"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_50pxv" android:layout_width="@dimen/_311pxh">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSelect"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_select"
                        style="@style/txtRobotosemibolditalic17" tools:text="@string/lbl_select"/>
                    <FrameLayout android:id="@+id/frameButtonVerify"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_50pxv"
                        android:layout_marginStart="@dimen/_1pxh" android:layout_width="@dimen/_310pxh">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtVerify"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:layout_marginEnd="@dimen/_134pxh"
                            android:layout_marginStart="@dimen/_134pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_verify"
                            style="@style/txtRobotosemibolditalic17" tools:text="@string/lbl_verify"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnDone"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_15pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_15pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_done"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded_3" tools:text="@string/lbl_done"/>
                    </FrameLayout>
                </FrameLayout>
            </FrameLayout>
        </LinearLayout>
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900"/>
                <LinearLayout
                    android:id="@+id/linearRowjobpreferences"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_128pxh"
                    android:layout_weight="0.95"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtJobPreferences"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.75"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_job_preferences"
                        style="@style/txtRobotosemibolditalic17_2" tools:text="@string/lbl_job_preferences"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSave"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_98pxh"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_save"
                        style="@style/txtRobotoromanregular17_4" tools:text="@string/lbl_save"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
    </FrameLayout>
</LinearLayout>

