<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Login"
    android:id="@+id/linearLogin"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="start"
    style="@style/groupStylewhite_A700">

    <ImageView
        android:id="@+id/imageRectangleSix"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="12dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/img_rectangle6" />

    <FrameLayout
        android:id="@+id/frameStackcreatefromfra"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_19pxv"
        android:layout_marginStart="@dimen/_33pxh"
        android:layout_marginEnd="@dimen/_33pxh"
        android:layout_marginTop="@dimen/_48pxv">

        <View
            android:id="@+id/viewCreatefromfra"
            android:layout_width="@dimen/_40pxh"
            android:layout_height="@dimen/_18pxv"
            android:layout_gravity="start"
            android:background="@color/white_A700" />

        <TextView
            android:id="@+id/txtEmail"
            style="@style/txtRobotoromanmedium16"
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:ellipsize="end"
            android:gravity="top|start"
            android:singleLine="true"
            android:text="@string/lbl_email" />


    </FrameLayout>
    <EditText
        android:background="@drawable/rectangle_bg_white_a700_border_gray_100_radius_4"
        android:ellipsize="end"
        android:hint="@string/lbl_mail_mail_com"
        android:id="@+id/etEmailOne"
        android:inputType="textEmailAddress"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_27pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_8pxv"
        android:layout_width="match_parent"
        android:paddingBottom="@dimen/_9pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingStart="@dimen/_16pxh"
        android:paddingTop="@dimen/_13pxv" android:singleLine="true"
        android:textColorHint="@color/gray_400_a2"
        style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
    <FrameLayout android:id="@+id/frameStackcreatefromfraOne"
        android:layout_height="@dimen/_19pxv"
        android:layout_marginEnd="@dimen/_32pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_29pxv"
        android:layout_width="@dimen/_71pxh" >
        <View android:background="@color/white_A700"
            android:id="@+id/viewCreatefromfraOne"
            android:layout_gravity="start"
            android:layout_height="@dimen/_18pxv"
            android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_71pxh"/>

        <TextView
            android:id="@+id/txtPassword"
            style="@style/txtRobotoromanmedium16"
            android:layout_width="204dp"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:ellipsize="end"
            android:gravity="top|start"
            android:singleLine="true"
            android:text="@string/lbl_password"
            tools:text="@string/lbl_password" />
    </FrameLayout>
    <FrameLayout android:id="@+id/frameStackeye"
        android:layout_gravity="center_horizontal"
        android:layout_height="@dimen/_40pxv"
        android:layout_marginEnd="@dimen/_26pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_356pxh">
        <ImageView android:id="@+id/imageEye"
            android:layout_gravity="bottom|end"
            android:layout_height="@dimen/_16pxh"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="@dimen/_18pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_eye"
            tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_border_gray_100_radius_4"
            android:drawableEnd="@drawable/img_music"
            android:drawablePadding="@dimen/_30pxh"
            android:ellipsize="end"
            android:hint="@string/lbl_password"
            android:id="@+id/etPasss"
            android:inputType="textPassword"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_10pxv"
            android:paddingEnd="@dimen/_12pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_12pxv"
            android:singleLine="true"
            android:textColorHint="@color/gray_400_a2"
            style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
    </FrameLayout>
    <LinearLayout
        android:id="@+id/linearRowrememberme"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_24pxv"
        android:layout_marginEnd="@dimen/_32pxh"
        android:orientation="horizontal"
        android:weightSum="1.0">

        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/checkBoxRememberme"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            android:buttonTint="@color/gray_100"
            android:ellipsize="end"
            android:fontFamily="@font/roboto"
            android:paddingStart="@dimen/_13pxh"
            android:paddingTop="@dimen/_4pxv"
            android:paddingBottom="@dimen/_5pxv"
            android:singleLine="true"
            android:text="@string/lbl_remember_me"
            android:textColor="@color/black_900"
            android:textFontWeight="400"
            android:textSize="@dimen/_10pxh"
            android:textStyle="normal"
            tools:text="@string/lbl_remember_me" />

        <TextView
            android:id="@+id/txtForgetPassword"
            style="@style/txtRobotoromanregular10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_marginBottom="@dimen/_4pxv"
            android:layout_weight="0.5"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@string/lbl_forget_password"
            android:gravity="end"
            tools:text="@string/lbl_forget_password" />
    </LinearLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:ellipsize="end" android:gravity="center"
        android:id="@+id/btnLogIn"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_26pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_25pxv"
        android:layout_width="match_parent"
        android:paddingBottom="@dimen/_11pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_11pxv" android:singleLine="true"
        android:text="@string/lbl_log_in"
        android:textAllCaps="false" tools:text="@string/lbl_log_in"
        style="@style/btnSolidRounded_3"/>
    <androidx.appcompat.widget.AppCompatButton
        android:ellipsize="end" android:gravity="center"
        android:id="@+id/btnCreateAccountOne"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_26pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_12pxv"
        android:layout_width="match_parent"
        android:paddingBottom="@dimen/_10pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_10pxv" android:singleLine="true"
        android:text="@string/lbl_create_account"
        android:textAllCaps="false"
        tools:text="@string/lbl_create_account"
        style="@style/btnSolidRounded_3"/>


    <LinearLayout
        android:id="@+id/linearRowgoogle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_46pxv"
        android:layout_marginEnd="@dimen/_32pxh"
        android:layout_marginBottom="@dimen/_1pxv"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <FrameLayout
            android:id="@+id/frameGoogle"
            style="@style/groupStylecornerRadius"
            android:layout_width="@dimen/_72pxh"
            android:layout_height="@dimen/_50pxv">

            <com.google.android.gms.common.SignInButton
                android:id="@+id/googleSignInButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>


        </FrameLayout>

        <FrameLayout
            android:id="@+id/frameFacebook"
            style="@style/groupStylecornerRadius"
            android:layout_width="@dimen/_72pxh"
            android:layout_height="@dimen/_50pxv"
            android:layout_marginStart="@dimen/_14pxh">

            <ImageView
                android:id="@+id/imageFacebook"
                android:layout_width="@dimen/_28pxh"
                android:layout_height="@dimen/_25pxh"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginBottom="@dimen/_12pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_facebook"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_facebook" />
        </FrameLayout>
    </LinearLayout>
</LinearLayout>

