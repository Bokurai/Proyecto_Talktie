<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Login"
    android:id="@+id/linearLogin"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    style="@style/groupStylewhite_A700">

    <ImageView
        android:id="@+id/imageRectangleSix"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="12dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/img_rectangle6" />

    <FrameLayout
        android:id="@+id/frameStackcreatefromfra"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_19pxv"
        android:layout_marginStart="@dimen/_33pxh"
        android:layout_marginEnd="@dimen/_33pxh"
        android:layout_marginTop="@dimen/_48pxv">

        <View
            android:id="@+id/viewCreatefromfra"
            android:layout_width="@dimen/_40pxh"
            android:layout_height="@dimen/_18pxv"
            android:layout_gravity="start"
            android:background="@color/white_A700" />

        <TextView
            android:id="@+id/txtEmail"
            style="@style/txtRobotoromanmedium16"
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:ellipsize="end"
            android:gravity="top|start"
            android:singleLine="true"
            android:text="@string/lbl_email" />


    </FrameLayout>
    <EditText
        android:background="@drawable/rectangle_bg_white_a700_border_gray_100_radius_4"
        android:ellipsize="end"
        android:id="@+id/emailEditText"
        android:inputType="textEmailAddress"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_27pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_8pxv"
        android:layout_width="match_parent"
        android:paddingBottom="@dimen/_9pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingStart="@dimen/_16pxh"
        android:paddingTop="@dimen/_13pxv" android:singleLine="true"
        android:hint="@string/lbl_mail_mail_com"
        style="@style/etSolidRounded_3"
        android:textColorHint="@color/gray_400_a2"
        tools:ignore="Autofill"/>
    <FrameLayout android:id="@+id/frameStackcreatefromfraOne"
        android:layout_height="@dimen/_19pxv"
        android:layout_marginEnd="@dimen/_32pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_29pxv"
        android:layout_width="@dimen/_71pxh" >
        <View android:background="@color/white_A700"
            android:id="@+id/viewCreatefromfraOne"
            android:layout_gravity="start"
            android:layout_height="@dimen/_18pxv"
            android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_71pxh"/>

        <TextView
            android:id="@+id/txtPassword"
            style="@style/txtRobotoromanmedium16"
            android:layout_width="204dp"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:ellipsize="end"
            android:gravity="top|start"
            android:singleLine="true"
            android:text="@string/lbl_password"
            tools:text="@string/lbl_password" />
    </FrameLayout>
    <FrameLayout android:id="@+id/frameStackeye"
        android:layout_gravity="center_horizontal"
        android:layout_height="@dimen/_40pxv"
        android:layout_marginEnd="@dimen/_26pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_356pxh">

        <EditText
            android:background="@drawable/rectangle_bg_white_a700_border_gray_100_radius_4"
            android:drawablePadding="@dimen/_30pxh"
            android:ellipsize="end"
            android:hint="@string/lbl_password"
            android:id="@+id/passwordEditText"
            android:inputType="textPassword"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_10pxv"
            android:paddingEnd="@dimen/_12pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_12pxv"
            android:singleLine="true"
            style="@style/etSolidRounded_3"
            android:textColorHint="@color/gray_400_a2"
            tools:ignore="Autofill"/>
    </FrameLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:ellipsize="end" android:gravity="center"
        android:id="@+id/btnLogIn"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_26pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_25pxv"
        android:layout_width="match_parent"
        android:paddingBottom="@dimen/_11pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_11pxv" android:singleLine="true"
        android:text="@string/lbl_log_in"
        android:textAllCaps="false" tools:text="@string/lbl_log_in"
        style="@style/btnSolidRounded_3"/>
    <androidx.appcompat.widget.AppCompatButton
        android:ellipsize="end" android:gravity="center"
        android:id="@+id/btnCreateAccountOne"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_26pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_12pxv"
        android:layout_width="match_parent"
        android:paddingBottom="@dimen/_10pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_10pxv" android:singleLine="true"
        android:text="@string/lbl_create_account"
        android:textAllCaps="false"
        style="@style/btnSolidRounded_3"/>


    <com.google.android.gms.common.SignInButton
        android:id="@+id/googleSignInButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/_12pxv" />


</LinearLayout>

