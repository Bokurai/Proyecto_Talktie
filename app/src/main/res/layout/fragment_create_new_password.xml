<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearColumnarrowleft"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical" style="@style/groupStylewhite_A700">
    <LinearLayout android:id="@+id/linearIPhone1314"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageArrowleft"
            android:layout_gravity="start"
            android:layout_height="@dimen/_12pxh"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="@dimen/_16pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowleft"
            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
        <ImageView android:id="@+id/imageRectangleSix"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_127pxh"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="@dimen/_142pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_rectangle6"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle6"/>
        <LinearLayout android:id="@+id/linearColumnframeseven"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtFrameSeven"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:paddingTop="@dimen/_1pxv"
                android:singleLine="true"
                android:text="@string/msg_create_new_pass"
                style="@style/txtSolid" tools:text="@string/msg_create_new_pass"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEntreyourpass"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/msg_entre_your_pass"
                style="@style/txtRobotoromanregular14_1" tools:text="@string/msg_entre_your_pass"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowframesevenOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <View android:background="@color/white_A700"
                    android:id="@+id/lineFrameSevenOne"
                    android:layout_height="@dimen/_20pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.0" android:layout_width="0dp"/>
                <FrameLayout
                    android:id="@+id/frameStackcreatefromfra"
                    android:layout_height="@dimen/_19pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_width="@dimen/_71pxh" style="@style/groupStylewhite_A700">
                    <View android:background="@color/white_A700"
                        android:id="@+id/viewCreatefromfra"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_18pxv"
                        android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_71pxh"/>

                    <TextView
                        android:id="@+id/txtPassword"
                        style="@style/txtRobotoromanmedium16"
                        android:layout_width="163dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:ellipsize="end"
                        android:gravity="top|start"
                        android:singleLine="true"
                        android:text="@string/lbl_password"
                        tools:text="@string/lbl_password" />
                </FrameLayout>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStackeye"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_47pxv"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_12pxv" android:layout_width="@dimen/_366pxh">
                <ImageView android:id="@+id/imageEye"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_eye"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_radius_4"
                    android:drawableEnd="@drawable/img_music"
                    android:drawablePadding="@dimen/_30pxh"
                    android:ellipsize="end"
                    android:hint="@string/msg_create_a_secure"
                    android:id="@+id/etInputField"
                    android:inputType="textPassword"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_9pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/gray_400_a2"
                    style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFrameSevenTwo"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:paddingTop="@dimen/_1pxv"
                android:singleLine="true"
                android:text="@string/msg_confirm_your_pa"
                style="@style/txtSolid" tools:text="@string/msg_confirm_your_pa"/>
            <FrameLayout android:id="@+id/frameStackinputfieldOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_47pxv"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_366pxh">
                <FrameLayout android:id="@+id/frameStackeyeOne"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_18pxv"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_13pxv" android:layout_width="@dimen/_18pxh">
                    <ImageView android:id="@+id/imageEyeOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_eye"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
                    <FrameLayout android:id="@+id/frameStackeyeTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_18pxv" android:layout_width="@dimen/_18pxh">
                        <ImageView android:id="@+id/imageEyeTwo"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_eye"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
                        <ImageView android:id="@+id/imageEyeThree"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_eye"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
                    </FrameLayout>
                </FrameLayout>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_100_radius_4"
                    android:drawableEnd="@drawable/img_music"
                    android:drawablePadding="@dimen/_30pxh"
                    android:ellipsize="end"
                    android:hint="@string/msg_create_a_secure"
                    android:id="@+id/etInputFieldOne"
                    android:inputType="textPassword"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_9pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/gray_400_a2"
                    style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            </FrameLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnSave"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_121pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_13pxv"
                android:singleLine="true"
                android:text="@string/lbl_save"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_3" tools:text="@string/lbl_save"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

