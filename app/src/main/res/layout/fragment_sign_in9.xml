                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingStart="@dimen/_8pxh"
                    android:paddingTop="@dimen/_1pxv"
                    android:singleLine="true"
                    android:text="@string/msg_i_am_currently"
                    android:textColor="@color/gray_701"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_15pxh"
                    android:textStyle="normal" tools:text="@string/msg_i_am_currently"/>
