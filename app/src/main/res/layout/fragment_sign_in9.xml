                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.67"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_experience"
                        style="@style/txtRobotosemibolditalic17_2" tools:text="@string/lbl_experience"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSave"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_115pxh"
                        android:layout_weight="0.33"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_save"
                        style="@style/txtRobotoromanregular17_4" tools:text="@string/lbl_save"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
    </FrameLayout>
</LinearLayout>
