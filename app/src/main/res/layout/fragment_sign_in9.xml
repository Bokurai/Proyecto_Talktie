                    android:id="@+id/etGroup114"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/black_900"
                    style="@style/etSolidRounded_3" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCompany"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_company"
                    style="@style/txtRobotosemibolditalic17_1" tools:text="@string/lbl_company"/>
                <EditText
                    android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_company_n"
                    android:id="@+id/etGroup113"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/bluegray_400"
                    style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLocation"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_location"
                    style="@style/txtRobotosemibolditalic17_1" tools:text="@string/lbl_location"/>
                <EditText
                    android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_company_l"
                    android:id="@+id/etGroup112"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/bluegray_400"
                    style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
