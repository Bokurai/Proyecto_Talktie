                        android:text="@string/lbl_add_more"
                        style="@style/txtRobotoromanregular13_1" tools:text="@string/lbl_add_more"/>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout
                android:id="@+id/frameButtonContinue"
                android:layout_width="@dimen/_311pxh"
                android:layout_height="@dimen/_50pxv"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_55pxh"
                android:layout_marginEnd="@dimen/_13pxh">

