                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnDdmmyyyyOne"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_23pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_12pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_dd_mm_yyyy"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded_5" tools:text="@string/lbl_dd_mm_yyyy"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
