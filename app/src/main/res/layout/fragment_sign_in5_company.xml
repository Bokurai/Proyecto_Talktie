<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    tools:context=".signIn5_company"
    android:gravity="center_horizontal"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/signIn5_company"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical" style="@style/groupStylewhite_A700">
    <FrameLayout android:gravity="start"
        android:id="@+id/frame13PersonalInf"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">

        <LinearLayout
            android:id="@+id/linearColumnone"
            style="@style/groupStylewhite_A700"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginEnd="@dimen/_10pxh"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/linearNumber"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_60pxh"
                android:layout_marginEnd="@dimen/_40pxh"
                android:gravity="end"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearColumntwo1"
                    style="@style/groupStylelight_green_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtTwo1"
                        style="@style/txtRobotoromanmedium15_2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="1"
                        tools:text="1" />
                </LinearLayout>

                <View
                    android:id="@+id/viewLine"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:background="@color/light_green_A700" />

                <LinearLayout
                    android:id="@+id/linearColumntwo"
                    style="@style/groupStylelight_green_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtTwo"
                        style="@style/txtRobotoromanmedium15_2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_2"
                        tools:text="@string/lbl_2" />
                </LinearLayout>

                <View
                    android:id="@+id/viewLineOne"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:background="@color/light_green_A700" />

                <LinearLayout
                    android:id="@+id/linearColumnthree"
                    style="@style/groupStylelight_green_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtThree"
                        style="@style/txtRobotoromanmedium15_2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_3"
                        tools:text="@string/lbl_3" />
                </LinearLayout>

                <View
                    android:id="@+id/viewLineTwo"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:background="@color/light_green_A700" />

                <LinearLayout
                    android:id="@+id/linearColumnfour"
                    style="@style/groupStylelight_green_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtFour"
                        style="@style/txtRobotoromanmedium15_2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_4"
                        tools:text="@string/lbl_4" />
                </LinearLayout>

                <View
                    android:id="@+id/viewLineThree"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:background="@color/light_green_A700" />

                <LinearLayout
                    android:id="@+id/linearColumnfour2"
                    style="@style/groupStylewhite_4"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.12"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btnMaximize"
                        android:layout_width="@dimen/_32pxh"
                        android:layout_height="@dimen/_32pxh"
                        android:background="@drawable/rectangle_bg_white_a700_border_light_green_a700_radius_16"
                        android:paddingHorizontal="@dimen/_9pxh"
                        android:paddingVertical="@dimen/_9pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_maximize"
                        tools:ignore="ContentDescription" />
                </LinearLayout>

            </LinearLayout>

            <TextView
                android:id="@+id/txtPhone"
                style="@style/txtRobotosemibolditalic17_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginStart="@dimen/_64pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_marginEnd="@dimen/_64pxh"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                tools:text="@string/lbl_summary_company" />

            <EditText
                android:id="@+id/etNameSignOne"
                style="@style/etSolidRounded_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                android:ellipsize="end"
                android:inputType="text"
                android:hint="Description of the company"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:singleLine="true"
                android:textColorHint="@color/bluegray_900"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/email"
                style="@style/txtRobotosemibolditalic17_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginStart="@dimen/_64pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_64pxh"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                tools:text="@string/lbl_foundation" />

            <EditText
                android:id="@+id/etcodecompany"
                style="@style/etSolidRounded_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                android:ellipsize="end"
                android:hint="DD/MM/YYYY"
                android:inputType="phone"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_12pxv"
                android:singleLine="true"
                android:textColorHint="@color/bluegray_400"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/sectorcompany"
                style="@style/txtRobotosemibolditalic17_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginStart="@dimen/_64pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_64pxh"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                tools:text="@string/lbl_specialities_company" />

            <EditText
                android:id="@+id/etsectorcompany"
                style="@style/etSolidRounded_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                android:ellipsize="end"
                android:hint="Specialities"
                android:inputType="textEmailAddress"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:singleLine="true"
                android:textColorHint="@color/bluegray_900"
                tools:ignore="Autofill" />


            <FrameLayout
                android:id="@+id/frameButtonContinue"
                android:layout_width="@dimen/_311pxh"
                android:layout_height="@dimen/_50pxv"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_75pxh"
                android:layout_marginEnd="@dimen/_13pxh">

                <TextView
                    android:id="@+id/txtContinue"
                    style="@style/txtRobotosemibolditalic17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_continue"
                    tools:text="@string/lbl_continue" />

                <FrameLayout
                    android:id="@+id/frameButtonSelect"
                    android:layout_width="@dimen/_311pxh"
                    android:layout_height="@dimen/_50pxv"
                    android:layout_gravity="center">


                    <FrameLayout
                        android:id="@+id/frameButtonVerify"
                        android:layout_width="@dimen/_310pxh"
                        android:layout_height="@dimen/_50pxv"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/_1pxh">

                        <androidx.appcompat.widget.AppCompatButton
                            android:id="@+id/btnContinueOne"
                            style="@style/btnSolidRounded_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingBottom="@dimen/_15pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_continue"
                            android:textAllCaps="false"
                            tools:text="@string/lbl_continue" />
                    </FrameLayout>
                </FrameLayout>
            </FrameLayout>
        </LinearLayout>
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900"/>
                <LinearLayout
                    android:id="@+id/linearRowpersonalinform"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_114pxh"
                    android:layout_weight="0.95"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPersonalInform"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.79"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        style="@style/txtRobotosemibolditalic17_2"
                        tools:text="@string/lbl_location_company"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSave"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_81pxh"
                        android:layout_weight="0.21"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_save"
                        style="@style/txtRobotoromanregular17_4" tools:text="@string/lbl_save"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
    </FrameLayout>
</LinearLayout>
