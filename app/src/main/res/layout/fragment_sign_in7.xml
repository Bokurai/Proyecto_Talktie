<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SignIn7"
    android:gravity="start"
    android:id="@+id/linearColumnarrowleft"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    style="@style/groupStylewhite_A700">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarToolbar"
        android:layout_marginStart="@dimen/_10pxh"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">

        <LinearLayout android:id="@+id/linearRowarrowleft"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_12pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_9pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent" android:orientation="horizontal">

            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_21pxh"
                android:layout_width="@dimen/_15pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft_black_900"
                tools:ignore="ContentDescription" />

            <LinearLayout
                android:id="@+id/linearRowlocation"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_125pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_weight="0.94"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtLocation"
                    style="@style/txtRobotosemibolditalic17_2"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_location"
                    tools:text="@string/lbl_location" />

            </LinearLayout>
        </LinearLayout>


    </androidx.appcompat.widget.Toolbar>

    <ScrollView android:clipToPadding="false"
        android:fillViewport="true"
        android:id="@+id/scrollViewScrollview"
        android:layout_height="0dp"
        android:layout_marginEnd="@dimen/_10pxh"
        android:layout_weight="1" android:layout_width="match_parent">


        <FrameLayout android:id="@+id/frame14Location"
            android:layout_height="@dimen/_737pxv"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">

            <LinearLayout
                android:id="@+id/linearColumnone"
                style="@style/groupStylewhite_A700"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="start"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/linearNumber"
                    android:layout_width="298dp"
                    android:layout_height="44dp"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_21pxh"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:gravity="end"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/linearColumnZero"
                        style="@style/groupStylelight_green_2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.12"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtOne"
                            style="@style/txtRobotoromanmedium15_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.12"
                            android:ellipsize="end"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:paddingStart="@dimen/_12pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_13pxh"
                            android:paddingBottom="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_1"
                            android:textAlignment="center"
                            tools:text="@string/lbl_1" />
                    </LinearLayout>


                    <View
                        android:id="@+id/viewLine"
                        android:layout_width="@dimen/_24pxh"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:background="@color/light_green_A700" />

                    <LinearLayout
                        android:id="@+id/linearColumntwo"
                        style="@style/groupStylelight_green_2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.12"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtTwo"
                            style="@style/txtRobotoromanmedium15_2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_2"
                            tools:text="@string/lbl_2" />
                    </LinearLayout>

                    <View
                        android:id="@+id/viewLineOne"
                        android:layout_width="@dimen/_24pxh"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:background="@color/light_green_A700" />

                    <LinearLayout
                        android:id="@+id/linearColumnthree"
                        style="@style/groupStylewhite_4"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.12"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtThree"
                            style="@style/txtSolidRounded_2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_3"
                            tools:text="@string/lbl_3" />
                    </LinearLayout>

                    <View
                        android:id="@+id/viewLineTwo"
                        android:layout_width="@dimen/_24pxh"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:background="@color/light_green_A700" />

                    <LinearLayout
                        android:id="@+id/linearColumnfour"
                        style="@style/groupStylewhite_4"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.12"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtFour"
                            style="@style/txtSolidRounded_2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_4"
                            tools:text="@string/lbl_4" />
                    </LinearLayout>

                    <View
                        android:id="@+id/viewLineThree"
                        android:layout_width="@dimen/_24pxh"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:background="@color/light_green_A700" />

                    <LinearLayout
                        android:id="@+id/linearColumnfive"
                        style="@style/groupStylewhite_4"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.12"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <ImageButton
                            android:id="@+id/btnMaximize"
                            android:layout_width="@dimen/_32pxh"
                            android:layout_height="@dimen/_32pxh"
                            android:background="@drawable/rectangle_bg_white_a700_border_light_green_a700_radius_16"
                            android:paddingHorizontal="@dimen/_9pxh"
                            android:paddingVertical="@dimen/_9pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_maximize"
                            tools:ignore="ContentDescription" />
                    </LinearLayout>

                </LinearLayout>


                <TextView
                    android:id="@+id/txtCountry"
                    style="@style/txtRobotosemibolditalic17_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_marginEnd="@dimen/_52pxh"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_country"
                    tools:text="@string/lbl_country" />

                <EditText
                    android:id="@+id/etCountry"
                    style="@style/etSolidRounded_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/enter_country_of_origin"
                    android:inputType="text"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_12pxv"
                    android:singleLine="true"
                    android:textColor="@android:color/black"
                    android:textColorHint="@color/bluegray_400"
                    tools:ignore="Autofill" />

                <TextView
                    android:id="@+id/txtStreetAddress"
                    style="@style/txtRobotosemibolditalic17_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_52pxh"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_street_address"
                    tools:text="@string/lbl_street_address" />

                <EditText
                    android:id="@+id/etStreetAddress"
                    style="@style/etSolidRounded_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_street_ad"
                    android:inputType="text"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_12pxv"
                    android:singleLine="true"
                    android:textColor="@android:color/black"
                    android:textColorHint="@color/bluegray_400"
                    tools:ignore="Autofill" />

                <TextView
                    android:id="@+id/txtCityState"
                    style="@style/txtRobotosemibolditalic17_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_52pxh"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_city_state"
                    tools:text="@string/lbl_city_state" />

                <EditText
                    android:id="@+id/etCity"
                    style="@style/etSolidRounded_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_city_sta"
                    android:inputType="text"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:singleLine="true"
                    android:textColor="@android:color/black"
                    android:textColorHint="@color/bluegray_400"
                    tools:ignore="Autofill" />

                <TextView
                    android:id="@+id/txtPostalCode"
                    style="@style/txtRobotosemibolditalic17_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_52pxh"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_postal_code"
                    tools:text="@string/lbl_postal_code" />

                <EditText
                    android:id="@+id/etPostalCode"
                    style="@style/etSolidRounded_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_postal_co"
                    android:inputType="text"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:singleLine="true"
                    android:textColor="@android:color/black"
                    android:textColorHint="@color/bluegray_400"
                    tools:ignore="Autofill" />

                <FrameLayout
                    android:id="@+id/frameButtonContinue"
                    android:layout_width="@dimen/_311pxh"
                    android:layout_height="@dimen/_50pxv"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_75pxh"
                    android:layout_marginEnd="@dimen/_13pxh">

                    <TextView
                        android:id="@+id/txtContinue"
                        style="@style/txtRobotosemibolditalic17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_continue"
                        tools:text="@string/lbl_continue" />

                    <FrameLayout
                        android:id="@+id/frameButtonSelect"
                        android:layout_width="@dimen/_311pxh"
                        android:layout_height="@dimen/_50pxv"
                        android:layout_gravity="center">

                        <FrameLayout
                            android:id="@+id/frameButtonVerify"
                            android:layout_width="@dimen/_310pxh"
                            android:layout_height="@dimen/_50pxv"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_1pxh">

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/btnContinuetoEight"
                                style="@style/btnSolidRounded_3"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_15pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_15pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_continue"
                                android:textAllCaps="false"
                                tools:text="@string/lbl_continue" />
                        </FrameLayout>
                    </FrameLayout>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </ScrollView>
</LinearLayout>

