<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/login">

    <fragment
        android:id="@+id/first"
        android:name="com.example.proyecto_talktie.First"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" />
    <fragment
        android:id="@+id/selectRegister"
        android:name="com.example.proyecto_talktie.SelectRegister"
        android:label="fragment_select_register"
        tools:layout="@layout/fragment_select_register" />
    <fragment
        android:id="@+id/login"
        android:name="com.example.proyecto_talktie.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_forgetPassword"
            app:destination="@id/forgetPassword" />
    </fragment>
    <fragment
        android:id="@+id/forgetPassword"
        android:name="com.example.proyecto_talktie.ForgetPassword"
        android:label="fragment_forget_password"
        tools:layout="@layout/fragment_forget_password" >
        <action
            android:id="@+id/action_forgetPassword_to_codePassword"
            app:destination="@id/codePassword" />
    </fragment>
    <fragment
        android:id="@+id/codePassword"
        android:name="com.example.proyecto_talktie.CodePassword"
        android:label="fragment_code_password"
        tools:layout="@layout/fragment_code_password" >
        <action
            android:id="@+id/action_codePassword_to_createNewPassword"
            app:destination="@id/createNewPassword" />
    </fragment>
    <fragment
        android:id="@+id/createNewPassword"
        android:name="com.example.proyecto_talktie.CreateNewPassword"
        android:label="fragment_create_new_password"
        tools:layout="@layout/fragment_create_new_password" />
    <fragment
        android:id="@+id/signIn1"
        android:name="com.example.proyecto_talktie.SignIn1"
        android:label="fragment_sign_in1"
        tools:layout="@layout/fragment_sign_in1" />

    <fragment
        android:id="@+id/signIn6"
        android:name="com.example.proyecto_talktie.SignIn6"
        android:label="fragment_sign_in6"
        tools:layout="@layout/fragment_sign_in6" />
    <fragment
        android:id="@+id/signIn7"
        android:name="com.example.proyecto_talktie.SignIn7"
        android:label="fragment_sign_in7"
        tools:layout="@layout/fragment_sign_in7" />
    <fragment
        android:id="@+id/signIn8"
        android:name="com.example.proyecto_talktie.SignIn8"
        android:label="fragment_sign_in8"
        tools:layout="@layout/fragment_sign_in8" />
    <fragment
        android:id="@+id/signIn9"
        android:name="com.example.proyecto_talktie.SignIn9"
        android:label="fragment_sign_in9"
        tools:layout="@layout/fragment_sign_in9" />
    <fragment
        android:id="@+id/signIn10"
        android:name="com.example.proyecto_talktie.SignIn10"
        android:label="fragment_sign_in10"
        tools:layout="@layout/fragment_sign_in10" />
    <fragment
        android:id="@+id/profileStudent"
        android:name="com.example.proyecto_talktie.ProfileStudent"
        android:label="fragment_profile_student"
        tools:layout="@layout/fragment_profile_student" />
    <action
        android:id="@+id/action_goLogin"
        app:destination="@+id/login" />

    <action
        android:id="@+id/action_goSignIn1"
        app:destination="@+id/signIn1" />

    <action
        android:id="@+id/action_goSignIn2"
        app:destination="@+id/signIn2" />

    <action
        android:id="@+id/action_goSignIn3"
        app:destination="@+id/signIn3" />

    <action
        android:id="@+id/action_goSignIn4"
        app:destination="@+id/signIn4" />

    <action
        android:id="@+id/action_goSelectRegister"
        app:destination="@+id/selectRegister" />

    <action
        android:id="@+id/action_goForgetPassword"
        app:destination="@id/forgetPassword" />

    <action
        android:id="@+id/action_goCodePassword"
        app:destination="@id/codePassword" />

    <action
        android:id="@+id/action_goCreateNewPassword"
        app:destination="@id/createNewPassword" />
    <action
        android:id="@+id/action_to_Register"
        app:destination="@id/signIn1" />
    <action
        android:id="@+id/action_goHome"
        app:destination="@+id/home" />

    <action
        android:id="@+id/action_goOffer"
        app:destination="@id/offer"/>

    <action
        android:id="@+id/action_goOfferDetailsFragment"
        app:destination="@id/offerDetailsFragment"/>
    <action
        android:id="@+id/action_goCompanyProfile"
        app:destination="@+id/companyProfile" />
    <action
        android:id="@+id/action_sendRequest"
        app:destination="@+id/sendRequest2" />
    <fragment
        android:id="@+id/messagesHome"
        android:name="com.example.proyecto_talktie.messagesHome"
        android:label="fragment_messages_home"
        tools:layout="@layout/fragment_messages_home" />
    <fragment
        android:id="@+id/home"
        android:name="com.example.proyecto_talktie.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/offer"
        android:name="com.example.proyecto_talktie.Offer"
        android:label="fragment_offer"
        tools:layout="@layout/fragment_offer" />
    <fragment
        android:id="@+id/studentSearchView"
        android:name="com.example.proyecto_talktie.StudentSearchView"
        android:label="fragment_student_search_view"
        tools:layout="@layout/fragment_student_search_view" />
    <fragment
        android:id="@+id/companyProfile"
        android:name="com.example.proyecto_talktie.CompanyProfile"
        android:label="fragment_company_profile"
        tools:layout="@layout/fragment_company_profile" />
    <action
        android:id="@+id/action_goStudentSearchView"
        app:destination="@id/studentSearchView" />
    <fragment
        android:id="@+id/offerDetailsFragment"
        android:name="com.example.proyecto_talktie.OfferDetailsFragment"
        android:label="fragment_offer_details"
        tools:layout="@layout/fragment_offer_details" />
    <fragment
        android:id="@+id/sendRequest2"
        android:name="com.example.proyecto_talktie.sendRequest2"
        android:label="fragment_send_request2"
        tools:layout="@layout/fragment_send_request2" />
</navigation>