<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/login">

    <fragment
        android:id="@+id/first"
        android:name="com.example.proyecto_talktie.First"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" />
    <fragment
        android:id="@+id/selectRegister"
        android:name="com.example.proyecto_talktie.SelectRegister"
        android:label="fragment_select_register"
        tools:layout="@layout/fragment_select_register" />
    <fragment
        android:id="@+id/login"
        android:name="com.example.proyecto_talktie.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_forgetPassword"
            app:destination="@id/forgetPassword" />
    </fragment>
    <fragment
        android:id="@+id/forgetPassword"
        android:name="com.example.proyecto_talktie.ForgetPassword"
        android:label="fragment_forget_password"
        tools:layout="@layout/fragment_forget_password" >
        <action
            android:id="@+id/action_forgetPassword_to_codePassword"
            app:destination="@id/codePassword" />
    </fragment>
    <fragment
        android:id="@+id/codePassword"
        android:name="com.example.proyecto_talktie.CodePassword"
        android:label="fragment_code_password"
        tools:layout="@layout/fragment_code_password" >
        <action
            android:id="@+id/action_codePassword_to_createNewPassword"
            app:destination="@id/createNewPassword" />
    </fragment>
    <fragment
        android:id="@+id/createNewPassword"
        android:name="com.example.proyecto_talktie.CreateNewPassword"
        android:label="fragment_create_new_password"
        tools:layout="@layout/fragment_create_new_password" />
    <fragment
        android:id="@+id/signIn1"
